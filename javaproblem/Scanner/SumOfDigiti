import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    System.out.println("enter a number");
	    int a=sc.nextInt();
		int sum=0;
		while(a>0){
		    int last=a%10;
		    a=a/10;
		    sum=sum+last;
		}
		System.out.println(sum);
	}
}
