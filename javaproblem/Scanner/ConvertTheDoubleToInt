import java.util.Scanner;

public class DoubleToStr {

	public static void main(String[] args) {
		DoubletoStr1();
		StrtoDouble();
		

		

	}
	static void DoubletoStr1() {
		Scanner sc=new Scanner(System.in);
		double Number1=sc.nextDouble();
		System.out.println(String.valueOf(Number1));
	
		
		
	}
	static void StrtoDouble() {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the String It converts Into Double");
		String Number2=sc.next();
		double d=Double.parseDouble(Number2);
		System.out.println(d);
	}

}
