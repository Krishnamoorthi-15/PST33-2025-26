import java.util.Scanner;
public class Main

{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    System.out.println("enter a number:");
	    int strong=sc.nextInt();
	    int orginal=strong;
	    int sum=0;
	    while(strong>0){
	        int digit=strong%10;
	        int fact=1;
	        for(int i=1;i<=digit;i++){
	            fact=fact*i
	            
	        }
	        sum=sum+fact;
	        orginal=orginal/10;
	    }
		
	}
}
